<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <changeSet id="1" author="claude">
        <createTable tableName="old_table">
            <column name="id" type="INT"/>
        </createTable>
        <rollback>
            <dropTable tableName="old_table"/>
        </rollback>
    </changeSet>
    <changeSet id="2" author="claude">
        <renameTable oldTableName="old_table" newTableName="new_table"/>
        <rollback>
            <renameTable oldTableName="new_table" newTableName="old_table"/>
        </rollback>
    </changeSet>
</databaseChangeLog>